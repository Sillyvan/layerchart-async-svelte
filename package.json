		"tailwindcss": "^4.1.13",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.44.1",
		"vite": "^7.1.7",
		"wrangler": "^4.44.0",
		"@inlang/paraglide-js": "2.4.0"
	},
	"packageManager": "pnpm@10.8.1",
	"engines": {
		"node": ">=24.0.0"
